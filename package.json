{
  "name": "pure-es6-boilerplate",
  "version": "1.0.0",
  "description": "Standard ES6 Boilerplate configured with babel, webpack, jest and eslint.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:server lint:watch",
    "dev:server": "webpack-dev-server --hot --inline",
    "watch": "webpack -w -d",
    "package": "webpack -p",
    "build": "npm-run-all --parallel test package",
    "lint": "node_modules/.bin/esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest ./src"
  },
  "keywords": [
    "es6"
  ],
  "author": "James Curran",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^2.1.14",
    "jest": "^22.4.3",
    "npm-run-all": "^4.0.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
